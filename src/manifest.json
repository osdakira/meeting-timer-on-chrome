{
  "name": "meeting-timer-on-chrome",
  "description": "meeting-timer-on-chrome",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "https://github.com/",
    "https://trello.com/",
    "https://qiita.com/",
    "storage"
  ],
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34-stopwatch.png"
  },
  "icons": {
    "16": "icon-16-stopwatch.png",
    "48": "icon-48-stopwatch.png",
    "128": "icon-128-stopwatch.png"
  },
  "web_accessible_resources": ["injectedTimerElement.html"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*",
        "https://trello.com/*",
        "https://qiita.com/*"
      ],
      "run_at": "document_end",
      "js": ["myContentScript.bundle.js"]
    }
  ]
}
